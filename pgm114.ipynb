    "#04/08/23\n",
    "#copy n characters from mth position using negative indexing\n",
    "s1=input('enter string:')\n",
    "s2=''\n",
    "n=int(input('enter n:'))\n",
    "m=int(input('enter position(m):'))\n",
    "a=m+n\n",
    "i=m\n",
    "while i<a:\n",
    "    s2=s2+s1[-i]\n",
    "    i=i+1\n",
    "print(s2)"
   ]
