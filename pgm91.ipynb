    "#pg.no.91\n",
    "#28/7/23\n",
    "#print pascal pattern\n",
    "n=5\n",
    "c=1\n",
    "a=1\n",
    "while c<=5:\n",
    "    d=1\n",
    "    while d<=n:\n",
    "        print(' ',end=\"\")\n",
    "        d=d+1\n",
    "    d=1\n",
    "    while d<=a:\n",
    "        print('*',end=\"\")\n",
    "        d=d+1\n",
    "    print()\n",
    "    \n",
    "    a=a+2\n",
    "    n=n-1\n",
    "    c=c+1"
