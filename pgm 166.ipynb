    "#16/08/23\n",
    "#Read string convert uppercase to lowercase and store in another string\n",
    "s1=input()\n",
    "s2=''\n",
    "n=len(s1)\n",
    "for i in range(n):\n",
    "    x=ord(s1[i])\n",
    "    if x>=65 and x<=90:\n",
    "        y=x+32\n",
    "        s2+=chr(y)\n",
    "    else:\n",
    "        s2+=s1[i]\n",
    "print(s2)"
   ]
