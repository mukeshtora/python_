    "pg.no.338\n",
    "02/12/23\n",
    "Store a list of n random numbers into a binary file\n",
    "\"\"\"\n",
    "import random as rd\n",
    "import pickle\n",
    "\n",
    "n=int(input('n:'))\n",
    "l=[rd.randint(1,1000) for i in range(n)]\n",
    "f=open('file1.dat','wb')\n",
    "pickle.dump(l,f)\n",
    "f.close()"
