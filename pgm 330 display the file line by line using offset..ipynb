    "\"\"\"\n",
    "pg.no.330\n",
    "27/11/23\n",
    "display the file line by line using offset.\n",
    "\"\"\"\n",
    "f=open('sample.txt','r')\n",
    "d={}\n",
    "offset=0\n",
    "l=0\n",
    "for i in f:\n",
    "    l+=1\n",
    "    d[l]=[offset,i]\n",
    "    offset+=len(i)+1\n",
    "    \n",
    "for j in d.values():\n",
    "    f.seek(j[0])\n",
    "    print(f.readline())"
   ]
