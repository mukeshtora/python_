    "\"\"\"\n",
    "pg.no.286\n",
    "calculate biggest of three numbers using closure function\n",
    "05/11/23\n",
    "\"\"\"\n",
    "def big():\n",
    "    def biggest(a,b,c):\n",
    "        a= (a if a>c else c) if a>b else (b if b>c else c)\n",
    "        return a\n",
    "    return biggest\n",
    "x=big()\n",
    "print(x(10,20,35))"
