    "#07/08/23\n",
    "# sum of elements of the matrix\n",
    "m=[]\n",
    "i=0\n",
    "while i<3:\n",
    "    a=[]\n",
    "    j=0\n",
    "    while j<3:\n",
    "        n=int(input(\"enter n:\"))\n",
    "        a.append(n)\n",
    "        j=j+1\n",
    "    m.append(a)\n",
    "    i=i+1\n",
    "s=0\n",
    "i=0\n",
    "while i<3:\n",
    "    j=0\n",
    "    while j<=2:\n",
    "        s=s+m[i][j]\n",
    "        j=j+1\n",
    "    i=i+1\n",
    "print(s)"
   ]
