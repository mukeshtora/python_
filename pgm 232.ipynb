    "#pg.no.232, 23/08/23\n",
    "\"\"\"\n",
    "construct a list ofn random numbers, convert to tuple and find sum\n",
    "\"\"\"\n",
    "import random as rd\n",
    "n=int(input('n:'))\n",
    "a=[rd.randint(-100,100) for  i in range(n)]\n",
    "b=tuple(a)\n",
    "d=0\n",
    "for i in b:\n",
    "    d+=i\n",
    "print(d)"
