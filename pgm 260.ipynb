    "\"\"\"\n",
    "find the highest accurance of the words\n",
    "20/09/23\n",
    "\"\"\"\n",
    "l=input('sentence:')\n",
    "w=l.split()\n",
    "a=list(set(w))\n",
    "d={}\n",
    "for i in a:\n",
    "    d[i]=l.count(i)\n",
    "x=0\n",
    "k=0\n",
    "for i in d:\n",
    "    if d[i]>x:\n",
    "        x=d[i]\n",
    "        k=i\n",
    "print('highest frequency word:',k,x)"
